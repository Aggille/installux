<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.koutosoft.installux">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.BATTERY_STATS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Padrao">
        <activity
            android:name=".actPrincipal"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actConfig"
            android:label="@string/title_activity_act_config"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".actImportacao"
            android:label="@string/title_activity_act_importacao"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".actFicha"
            android:label="@string/title_activity_act_ficha"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".actEnvia"
            android:label="@string/title_activity_act_envia"></activity>
        <activity
            android:name=".actConsultaFichas"
            android:label="@string/title_activity_act_consulta_fichas"></activity>
        <activity
            android:name=".actAdmin"
            android:label="@string/title_activity_act_admin"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
